{
    "name": "iqomp/formatter",
    "description": "Object formatter and data filler",
    "type": "library",
    "license": "MIT",
    "version": "1.0.4",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/config": "^1.2"
    },
    "extra": {
        "iqomp/config": "iqomp/config/"
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\Formatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iqomp\\Formatter\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    }
}
